#include <stdio.h>
#include <stdlib.h>
#include <sys/wait.h>
#include <unistd.h>

int main() {

  pid_t pid = fork();
  int wstatus = 0;
  if (pid == 0) {
    execl("/usr/bin/ls","/usr/bin/ls" "-a", "-l", NULL);
  } else {
    waitpid(pid, &wstatus,0);
    if(WIFEXITED(wstatus)){
      printf("Child exited normally: %d\n", WEXITSTATUS(wstatus));
    }
  }
}
