#include <pthread.h>
#include <stdio.h>
#include <string.h>
#include <unistd.h>
void *my_thread(void *arg) {
  char *message = arg;
  printf("Inside a thread\n");
  printf("Message is: %s\n", message);
  return (void *)strlen(message);
}

int main() {
  pthread_t thread_ID;

  pthread_create(&thread_ID, NULL, my_thread, NULL);

  void * ans;
  pthread_join(thread_ID, &ans);
  long long len = (long long )ans;
  printf("DONE");
}
